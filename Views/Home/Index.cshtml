@{
    ViewData["Title"] = "Home Page";
}
<div class="home-container">
    <section class="section-row">
        <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
                <li data-target="#myCarousel" data-slide-to="3"></li>
            </ol>
            <div class="carousel-inner" role="listbox">
                <div class="item active">
                    <img src="~/images/carousel-06.jpg" alt="Make Up Model" class="carousel-img" />
                    @*<div class="carousel-caption" role="option">
                            <p>
                                Make up
                                <a class="btn btn-default" href="#">
                                    Learn More
                                </a>
                            </p>
                        </div>*@
                </div>
                <div class="item">
                    <img src="~/images/carousel-02.jpg" alt="Make Up Pallet" class="carousel-img" />

                </div>
                <div class="item">
                    <img src="~/images/carousel-3.jpg" alt="Make Up Model" class="carousel-img" />
                </div>
                <div class="item">
                    <img src="~/images/carousel-05.jpg" alt="Make Up Nail Model" class="carousel-img" />
                </div>
            </div>
            <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>

        <div class="delivery-signup-sctn">
            <form class="signup">
                <p class="signup-header">Newsletter</p>
                <input type="text" name="email" placeholder="email address" />
                <br />
                <input type="number" name="number" placeholder="phone number" />
                <br />
                <input type="submit" value="sign up now" />
            </form>
            <div class="delivery-container">
                <h6 class="delivery-header">
                    FREE standard delivery on all orders
                </h6>
                <a class="find-out-more" href="more">find out more</a>
            </div>
        </div>
    </section>

    <section class="section-row">
        <iframe style="width: 50%;" title="tutorial" src="https://www.youtube.com/embed/w3p3dBhQYko" frameBorder="0" allowfullscreen></iframe>
        <div class="img-with-overlay">
            <img src="~/images/carousel-07.jpg" alt="Avatar" class="image" />
            <div class="overlay">
                <div class="text">MakeApp</div>
            </div>
        </div>
    </section>

    <section class="section-row">
        <div class="trio-img-container">
            <img class="img" src="~/images/carousel-08.jpg" alt="pallete" />
            <img class="img" src="~/images/home-001.jpg" alt="eyes" />
            <img class="img" src="~/images/carousel-04.jpg" alt="purple" />
        </div>
    </section>
</div>